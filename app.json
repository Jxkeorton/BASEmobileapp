{
  "expo": {
    "name": "basemobile-app",
    "slug": "basemobile-app",
    "version": "1.0.0",
    "orientation": "portrait",
    "icon": "./assets/bitmap.png",
    "userInterfaceStyle": "light",
    "splash": {
      "image": "./assets/bitmap.png",
      "resizeMode": "contain",
      "backgroundColor": "#000000"
    },
    "assetBundlePatterns": [
      "**/*"
    ],
    "ios": {
      "supportsTablet": true,
      "bundleIdentifier": "com.jxkeorton.basemobileapp"
    },
    "android": {
      "adaptiveIcon": {
        "foregroundImage": "./assets/bitmap.png",
        "backgroundColor": "#ffffff"
      },
      "package": "com.jxkeorton.basemobileapp"
    },
    "web": {
      "favicon": "./assets/favicon.png",
      "bundler": "metro"
    },
    "plugins": [
      "expo-router"
    ],
    "scheme": "demo",
    "extra": {
      "router": {
        "origin": false
      },
      "eas": {
        "projectId": "28d69f8e-6084-4159-a419-2e40940ea865"
      }
    },
    "owner": "jxkeorton"
  }
}
